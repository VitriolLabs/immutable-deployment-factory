{
  "name": "@vitriollabs/immutable-deployment-factory",
  "version": "0.1.1",
  "description": "Deterministic omni-chain smart contract deployments",
  "scripts": {
    "slither": "slither . --filter-paths \"Contracts/\" --exclude assembly,unused-return,solc-version,low-level-calls,divide-before-multiply",
    "hint": "solhint 'Contracts/**/*.sol'",
    "lint": "prettier --list-different --plugin=prettier-plugin-solidity 'Contracts/**/*.sol'",
    "lint-fix": "prettier --write --plugin=prettier-plugin-solidity 'Contracts/**/*.sol'"
  },
  "keywords": ["solidity", "sol", "ethereum", "eth", "create2", "create3", "deployment"],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/VitriolLabs/immutable-deployment-factory.git"
  },
  "author": {
    "name": "Cameron White (Slvrfn)",
    "email": "c@meron.dev",
    "url": "https://ca.meron.dev/"
  },
  "license": "MIT",
  "dependencies": {
    "solady": "^0.0.161"
  },
  "devDependencies": {
    "@ethersproject/address": "^5.7.0",
    "@nomicfoundation/hardhat-chai-matchers": "^2.0.0",
    "@nomicfoundation/hardhat-ethers": "^3.0.0",
    "@nomicfoundation/hardhat-network-helpers": "^1.0.10",
    "@nomicfoundation/hardhat-toolbox": "^4.0.0",
    "@nomiclabs/hardhat-solhint": "^3.0.1",
    "@typechain/ethers-v6": "^0.5.0",
    "@typechain/hardhat": "^9.0.0",
    "@types/chai": "^4.2.0",
    "@types/mocha": "^10.0.6",
    "@types/node": "^20.10.5",
    "chai": "^4.2.0",
    "ethers": "^6.4.0",
    "hardhat": "^2.19.4",
    "hardhat-contract-sizer": "^2.10.0",
    "prettier": "^3.1.1",
    "prettier-plugin-solidity": "^1.3.1",
    "solidity-coverage": "^0.8.0",
    "ts-node": "^10.9.2",
    "typechain": "^8.3.0",
    "typescript": "^5.3.3"
  }
}
